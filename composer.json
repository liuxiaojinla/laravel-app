{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "basement-chat/basement-chat": "^1.1",
        "endroid/qr-code": "^5.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "nwidart/laravel-modules": "^10.0",
        "pusher/pusher-php-server": "^7.2",
        "qiniu/php-sdk": "^7.12",
        "xin/analysis": "^2.0",
        "xin/capsule": "^1.0",
        "xin/hint": "^1.1",
        "xin/laravel-fortify": "@dev",
        "xin/menu": "@dev",
        "xin/payment": "@dev",
        "xin/wechat": "@dev",
        "xin/robot": "^1.2",
        "xin/setting": "^2.2",
        "yansongda/pay": "^3.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel-lang/lang": "^13.4",
        "laravel/breeze": "^1.26",
        "laravel/sail": "^1.25",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "repositories": {
        "xin/php-menu": {
            "type": "path",
            "url": "../../Projects/PhpProjects/php-menu"
        },
        "xin/php-payment": {
            "type": "path",
            "url": "../../Projects/PhpProjects/php-payment"
        },
        "xin/php-wechat": {
            "type": "path",
            "url": "../../Projects/PhpProjects/php-wechat"
        },
        "xin/php-laravel-fortify": {
            "type": "path",
            "url": "../../Projects/PhpProjects/php-laravel-fortify"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Plugins\\": "plugins/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
